#include "stm32f1xx.h"
#include "uart.h"
#include "systick.h"


int main(void){

	uart2_tx_init();
	systick_1hz_interrupt();

	while(1){

	}

}

void systick_callback(void){
	printf("Second has passed... \n\r");
}

void Systick_Handler(void){
	systick_callback();
}


