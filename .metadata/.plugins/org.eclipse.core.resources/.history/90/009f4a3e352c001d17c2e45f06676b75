#include "ssd1306.h"
#include "systick.h"

#define DEVID		0x3C
#define CTL_COMMAND	0x00
#define DISP_FULL	0xA5
#define DISP_ON		0xAF
#define INVERSE		0xA7
#define HEIGHT		0x1F


void ssd1306_sendCommand(char* data){
	I2C1_burstWrite(DEVID, sizeof(data), data);
}

void ssd1306_sendData(char* data){
	I2C1_burstWrite(DEVID, sizeof(data), data)
}
