#include "ssd1306.h"
#include "systick.h"

#define DEVID		0x3C
#define CTL_COMMAND	0x00
#define DISP_FULL	0xA5
#define DISP_ON		0xAF
#define INVERSE		0xA7
#define HEIGHT		0x1F


void ssd1306_sendCommand(void){
	I2C1_burstWrite(char saddr, 0x00, int n, char* data)
}

void ssd1306_sendData(){
	I2C1_burstWrite(char saddr, 0x40, int n, char* data)
}
